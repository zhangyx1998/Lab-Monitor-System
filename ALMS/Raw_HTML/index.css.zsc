body
{
	-webkit-tap-highlight-color: rgba(255,255,255,0);
	-webkit-touch-callout: none;
	-webkit-user-select: none;
	-khtml-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
}

input::-webkit-outer-spin-button,  
input::-webkit-inner-spin-button{  
	-webkit-appearance: none !important;  
	margin: 0;  
} 

canvas
{
	-webkit-tap-highlight-color:rgba(255,255,255,0);
}

@font-face {
	font-family:'ZFont-regular';
	src: url('Fonts/ZFont/ZFont-regular.ttf') format('truetype'),
		 url('Fonts/ZFont/ZFont-regular.svg') format('svg'),
		 url('Fonts/ZFont/ZFont-regular.eot') format('embedded-opentype'),
		 url('Fonts/ZFont/ZFont-regular.woff') format('woff'),
		 url('Fonts/ZFont/ZFont-regular.woff2') format('woff2');
	font-weight: normal;
	font-style: normal;
}

